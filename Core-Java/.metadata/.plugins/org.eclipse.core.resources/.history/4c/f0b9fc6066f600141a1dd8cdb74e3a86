package threads;

public class MyRunnable implements Runnable {
    public String name;
    public Integer n;

    public MyRunnable(String name, Integer n) {
        this.name = name;
        this.n = n;
    }

    @Override
    public void run() {
        for (int i = 0; i < 10; i++) {
            System.out.println("Thread " + name + ": " + n);
            n.increment();
        }
    }
}
