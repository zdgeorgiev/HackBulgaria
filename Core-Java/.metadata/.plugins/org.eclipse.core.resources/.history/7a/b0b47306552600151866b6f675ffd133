import java.util.ArrayList;
import java.util.List;
import java.util.Scanner;

public class PowerSupply {

    private static List<Pair> connections = new ArrayList<Pair>();

    public static void main(String[] args) {

        Scanner s = new Scanner(System.in);
        int n = s.nextInt();

        ArrayList<ArrayList<Pair>> graph = new ArrayList<ArrayList<Pair>>();

        for (int i = 0; i < n; i++) {
            int firstV = s.nextInt();
            int secondV = s.nextInt();
            int weight = s.nextInt();

            Pair p1 = new Pair(firstV, weight);
            Pair p2 = new Pair(secondV, weight);

            graph.get(firstV).add(p1);
            graph.get(secondV).add(p2);
            connections.add(p);
        }
    }
}
