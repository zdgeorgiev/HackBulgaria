
public class CountSort {
    private static int[] numbers = new int[366];

    public static int[] sort(List<Integer> arr) {
        fillHistogram(arr);

        return numbers;
    }

    private static void fillHistogram(List<Integer> c) {
        for (int i = 0; i < c.size(); i++) {
            numbers[c.get(i)]++;
        }
    }
}
