package threads;

public class MyRunnable implements Runnable {
    public String name;
    public MyInteger i;

    public MyRunnable(String name) {
        this.name = name;
    }

    @Override
    public void run() {
        for (int i = 0; i < 2_000_000; i++) {
            System.out.println("Thread " + name + ": " + i);
            i.increment();
        }
    }

}
