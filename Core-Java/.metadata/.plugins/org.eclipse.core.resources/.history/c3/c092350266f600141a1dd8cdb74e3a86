package threads;

public class MyRunnable implements Runnable {
    public String name;
    public MyInteger n;

    public MyRunnable(String name) {
        this.name = name;
        n = new MyInteger();
    }

    @Override
    public void run() {
        for (int i = 0; i < 2_000_000; i++) {
            System.out.println("Thread " + name + ": " + n);
            n.increment();
        }
    }
}
