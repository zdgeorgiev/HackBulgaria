public class Node {

    private PhoneBook.Contact value;
    public PhoneBook.Contact left;
    public PhoneBook.Contact right;

    public Node(PhoneBook.Contact contact) {
        this.setValue(contact);
        this.left = null;
        this.right = null;
    }

    public PhoneBook.Contact getValue() {
        return value;
    }

    private void setValue(PhoneBook.Contact value) {
        this.value = value;
    }
}
