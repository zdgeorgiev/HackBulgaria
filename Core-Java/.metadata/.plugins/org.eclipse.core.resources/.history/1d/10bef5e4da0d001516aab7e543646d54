package queue;

public class QueueImpl implements Queue {

    private Node head;
    private int size;

    public QueueImpl() {
        this.head = null;
    }

    @Override
    public void push(Node value) {
        if (size != 0) {
            this.head.setNext(value);
        } else {
            this.head = value;
        }

        this.size++;
    }

    @Override
    public Node pop() {
        this.head.getNext().setNext(this.head.getNext());
        this.size--;
        return this.head;
    }

    @Override
    public Node peek() {
        return this.head;
    }

    @Override
    public int size() {
        return this.size;
    }
}
