package queue;

public class QueueImpl implements Queue {

    private Node head;
    private int size;

    public QueueImpl() {
        this.head = null;
    }

    @Override
    public void push(Node value) {
        if (size == 0) {
            this.head.setNext(value);
        }

    }

    @Override
    public Node pop() {
        this.head.getNext().setNext(this.head.getNext());
        return this.head;
    }

    @Override
    public Node peek() {
        return this.head;
    }

    @Override
    public int size() {
        return this.size;
    }
}
