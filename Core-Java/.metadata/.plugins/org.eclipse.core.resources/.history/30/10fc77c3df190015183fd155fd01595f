public class BST {

    public static class Node {

        public int value;
        public Node left;
        public Node right;
    }

    // Checks if a binary tree is a binary search tree.
    public boolean isBST(Node root) {
        
        if(root.value > root.left.value && root.value <= root.left.value) {
            isBST(root.left);
            isBST(root.right);
        }
        
        return false;
    }
}
