import java.util.ArrayList;
import java.util.Scanner;

public class PowerSupply {

    public static void main(String[] args) {

        Scanner s = new Scanner(System.in);
        int n = s.nextInt();

        ArrayList<ArrayList<Pair>> graph = new ArrayList<ArrayList<Pair>>();

        for (int i = 0; i < n; i++) {
            int firstV = s.nextInt();
            int secondV = s.nextInt();
            int weight = s.nextInt();

            Pair p1 = new Pair(secondV, weight);
            Pair p2 = new Pair(firstV, weight);

            if (graph.size() + 1 > firstV) {
                graph.get(firstV).add(p1);
            } else {
                while (graph.size() < firstV) {
                    graph.add(new ArrayList<Pair>());
                }

                graph.get(firstV - 1).add(p1);
            }

            if (graph.size() + 1 > secondV) {
                graph.get(secondV).add(p2);
            } else {
                while (graph.size() + 1 < secondV) {
                    graph.add(new ArrayList<Pair>());
                }

                graph.get(firstV - 1).add(p1);
            }
        }

        for (ArrayList<Pair> arrayList : graph) {
            for (Pair pair : arrayList) {
                System.out.print(pair);
            }
            System.out.println();
        }
    }
}
