import java.util.ArrayList;
import java.util.Scanner;

public class LongestSubsequence {

    private static RMQBinaryIndexedTree data;

    public static void main(String[] args) {
        Scanner s = new Scanner(System.in);

        int n = s.nextInt();
        data = new RMQBinaryIndexedTree(new ArrayList<Integer>(), n);

        for (int i = 0; i < n; i++) {
            int currentNumber = s.nextInt();

            if (currentNumber != 0) {
                data.set(currentNumber, data.max(0, currentNumber - 1) + 1);
            } else {
                data.set(0, 1);
            }
        }

        System.out.println(data.max(0, n));
    }
}
