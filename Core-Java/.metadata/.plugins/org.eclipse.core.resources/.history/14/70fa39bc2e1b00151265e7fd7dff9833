
public enum Packets {
    ICMP (6),
    UDP (5),
    RTM (4),
    IGMP (3),
    DNS (2),
    TCP (1);
    
    private int priority;
    
    private Packets(int priority) {
        this.setPriority(priority);
    }

    public int getPriority() {
        return priority;
    }

    public void setPriority(int priority) {
        this.priority = priority;
    }
}
