package testttttt;

import java.util.Scanner;

public class LostCostFlights {

    private static int[][] graph;
    private static int[][] minPaths;

    public static void main(String[] args) {

        Scanner s = new Scanner(System.in);

        int n = s.nextInt();
        graph = new int[n][n];

        for (int i = 0; i < n; i++) {
            for (int j = 0; j < n; j++) {
                graph[i][j] = s.nextInt();
            }
        }

        for (int i = 0; i < n; i++) {
            createMinPaths(i);
        }

        int commands = s.nextInt();

        for (int i = 0; i < commands; i++) {
            System.out.println(getMinPath(s.nextInt(), s.nextInt()));
        }

    }

    private static void createMinPaths(int startPoint) {
        // TODO Auto-generated method stub

    }

    private static int getMinPath(int from, int to) {
        return minPaths[from][to];
    }
}
