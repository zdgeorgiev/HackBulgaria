package ToDoList;

public class Program {
    public static void main(String[] args) {
        ToDoList todo = new ToDoList(11); // 11 hours remaining!
        todo.add(new StudyForAlgebraTask(10, 5)); // maximum priority!
        todo.add(new LearnGeometryTask(3)); // default priority, smaller than 10
        todo.add(new GoOutTask(1.5f)); // default priority, smaller than 10
        todo.add(new SleepTask(5)); // straight 8 hours, of course!
    }
}
