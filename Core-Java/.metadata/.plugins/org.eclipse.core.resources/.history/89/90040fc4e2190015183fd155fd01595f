public class BST {

    public static class Node {

        public int value;
        public Node left;
        public Node right;
    }

    // Checks if a binary tree is a binary search tree.
    public boolean isBST(Node root) {
        return this.isValid;
    }
    
    private boolean isValid = false;
    
    private void DFS(Node root, int parentValue) {
        
        if(root == null) {
            return;
        }
        

        
        int parentValue = 
        Node leftChild = root.left;
        Node rightChild = root.right;

        if (leftChild != null) {
            if (!isBST(leftChild)) {
                return false;
            }
        }

        if (rightChild != null && rightChild.value < parentValue) {
            if (!isBST(rightChild)) {
                return false;
            }
        }

        return true;
    }
}