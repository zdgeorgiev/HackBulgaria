package brackets;

public class BracketsHolder {
    private boolean isRoundBracketAvaible;
    private boolean isSquareBracketAvaible;
    private boolean isCurlyBracketAvaible;

    public BracketsHolder(boolean roundBracketAvailable, boolean squareBracketAvailabel, boolean curclyBracketAvailable) {
        this.setRoundBracketAvaible(roundBracketAvailable);
        this.isSquareBracketAvaible = squareBracketAvailabel;
        this.isCurlyBracketAvaible = curclyBracketAvailable;
    }

    public boolean isRoundBracketAvaible() {
        return isRoundBracketAvaible;
    }

    public void setRoundBracketAvaible(boolean isRoundBracketAvaible) {
        this.isRoundBracketAvaible = isRoundBracketAvaible;
    }
}
