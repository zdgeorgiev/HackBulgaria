package sorting;

import java.util.ArrayList;

public class SelectionSort {

    public static void sort(ArrayList<Integer> c) {

        for (int i = 0; i < c.size() - 1; i++) {
            int minIndex = i;

            for (int j = i + 1; j < c.size(); j++) {

                if (c.get(i) < c.get(j)) {
                    minIndex = j;
                }
            }
        }
    }
}
