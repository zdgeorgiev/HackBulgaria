import java.util.ArrayList;
import java.util.List;

public class KLists {

    private Heap heap;

    public class Node {
        public int value;
        public Node next;
    }

    // Merge K sorted lists.
    public Node merge(List<Node> lists) {

        Node head = lists.get(0);
        
        ArrayList<Integer> currentList = new ArrayList<Integer>();
        
        while(head !=  null) {
            currentList.add(head.value);
            head = head.next;
        }

        return null;
    }
}
