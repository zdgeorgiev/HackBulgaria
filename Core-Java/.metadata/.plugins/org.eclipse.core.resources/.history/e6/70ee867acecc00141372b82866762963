package com.hackbulgaria.corejava;

public class Problems2Factory {
    String OATH_TOKEN = "THIS IS MY DEAR PASSWORD";

    public static Problems2 createProblems2Instance() {
        return new Problems2Impl();
    }

    public static void main(String[] args) {
        Problems2Impl p = new Problems2Impl();
        
        System.out.print(p.isOdd(10) + "\n");
        System.out.print(p.isPrime(15) + "\n");
        System.out.print(p.min(5, 6, 3, 5, 6) + "\n");
        System.out.print(p.kthMin(2, new int[] {5, 6, 3, 4, 1, 7, 11, 0}) + "\n");
        System.out.print(p.getAverage(new int[] {5, 6, 3, 4, 2, 7, 11, 2}) + "\n");
        System.out.print(p.doubleFac(3) + "\n");
        System.out.print(p.getSmallestMultiple(20) + "\n");
        System.out.print(p.getLargestPalindrome(10_0000) + "\n");
        
    }

