import java.util.Scanner;

public class LongestCommonSubstring {

    private static int[][] dp;

    private static int longestLenght = 0;
    private static int[] positionOfMax = new int[2];

    public static void main(String[] args) {

        Scanner s = new Scanner(System.in);

        String firstText = s.next();
        String secondText = s.next();

        dp = new int[secondText.length() + 1][firstText.length() + 1];

        for (int i = 0; i < dp.length; i++) {
            
            char secondTextChar = secondText.charAt(i);
            
            for (int j = 0; j < dp[i].length; j++) {

                char firstTextChar = firstText.charAt(j);
                
                if(firstTextChar == secondTextChar) {
                    dp[i][j] = dp[i - 1][j - 1] + 1;
                    
                    if(dp[i][j] > longestLenght) {
                        longestLenght = dp[i][j];
                        positionOfMax[0] = i;
                        positionOfMax[1] = j;
                    }
                }
            }
        }
    }
}
