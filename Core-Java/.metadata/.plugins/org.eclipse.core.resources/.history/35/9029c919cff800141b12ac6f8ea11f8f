package blockQueue;

public class MyRunnable implements Runnable {
    private String name;
    private BlockQueue<Integer> bq = new BlockQueue<Integer>();

    public MyRunnable(String name, BlockQueue<Integer> bq) {
        this.name = name;
        this.bq = bq;
    }

    @Override
    public void run() {
        System.out.println("Thread " + this.name + " - " + this.bq.poll());
    }
}
